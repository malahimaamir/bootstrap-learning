{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "script[type=importmap]"
        ]
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "user-select",
          "backdrop-filter"
        ]
      }
    ],
    "css-prefix-order": "off",
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ]
  }
}